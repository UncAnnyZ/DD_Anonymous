{
	"pages": [
		{
			"path": "pages/index/index",		// 主页
			"style": {
				"app-plus": {
					"titleNView": false
				}
			}
		},
		{ "path": "pages/test/test" },			// 测试页面
		{ "path": "pages/subNVue/popup" },		// 自定义弹窗
		{ "path": "pages/subNVue/Npopup" },		// 自定义弹窗
		{ "path": "components/popup/popup" },	// 自定义弹窗
		
		{
			"path": "pages/found/found",		// 发现页（广场页）
			"style": {
				"app-plus": {
					"titleNView": false	 //禁用原生导航栏
				}
			}
		},
		{
			"path": "pages/message/message",	// 消息页
			"style": {
				"navigationBarTitleText":"消息",
				"app-plus": {
					"titleNView": true //采用原生导航栏
				}
			}
		},
		{
			"path": "pages/mine/mine",			// 个人页
			"style": {
				"enablePullDownRefresh": false,
				"app-plus": {
					"titleNView": false
				}
			}
		},
		{
			"path": "pages/login/login",		// 登录页
			"style": {
				"navigationBarTitleText": "登录",
				"enablePullDownRefresh": false,
				"app-plus": {
					"titleNView": true,
					"background": "#efeff4",
					"subNVues": [{
						"id": "popup", // 唯一标识  
						"path": "pages/subNVue/popup", // 页面路径  
						"type": "popup",
						"style": {
							"type": "popup",
							"mask": "rgba(0,0,0,0.5)",
							"position": "absolute",
							"background": "transparent"
						}
					}]
				}
			}
		}, 
		{
			"path": "pages/setting/setting",	// 设置页
			"style": {
				"navigationBarTitleText": "设置",
				"app-plus": {
					"titleNView": true
				}
			}
		},
		{
			"path": "pages/search/search",		// 搜索页
			"style": {
				"app-plus": {
					"titleNView": false
				}
			}
		},
		{
			"path": "pages/agreement/agreement",// 授权页
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}
		},
		{
            "path" : "pages/index/save",		// 书签页
            "style" : {
                "navigationBarTitleText": "书签",
				"app-plus": {
					"titleNView": true,
					"background": "#efeff4",
					"subNVues": [{
						"id": "popup", // 唯一标识  
						"path": "pages/subNVue/popup", // 页面路径  
						"type": "popup",
						"style": {
							"type": "popup",
							"mask": "rgba(0,0,0,0.5)",
							"position": "absolute",
							"background": "transparent"
						}
					}]
				}
            }
        }
    ],
	
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "DDDD",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},

	"tabBar": {
		"list": [{
				"pagePath": "pages/index/index",
				"iconPath": "static/icon/tab_icon_index.png",
				"selectedIconPath": "static/icon/tab_icon_indexed.png"
			},
			{
				"pagePath": "pages/found/found",
				"iconPath": "static/icon/tab_icon_found.png",
				"selectedIconPath": "static/icon/tab_icon_founded.png"
			},
			{
				"pagePath": "pages/message/message",
				"iconPath": "static/icon/tab_icon_msg.png",
				"selectedIconPath": "static/icon/tab_icon_msged.png"
			},
			{
				"pagePath": "pages/mine/mine",
				"iconPath": "static/icon/tab_icon_mine.png",
				"selectedIconPath": "static/icon/tab_icon_mined.png"
			}
		]
	},
	"condition": { //模式配置，仅开发期间生效
		"current": 0, //当前激活的模式(list 的索引项)
		"list": [{
			"name": "", //模式名称
			"path": "", //启动页面，必选
			"query": "" //启动参数，在页面的onLoad函数里面得到
		}]
	}
}
